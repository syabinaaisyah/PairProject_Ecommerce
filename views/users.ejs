<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
<a href="/admin/dashboard" class="btn btn-primary">Back to Admin Dashboard</a>

<h1>All Users</h1>
<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Gender</th>
        </tr>
    </thead>
    <tbody>
        <% users.forEach(user => { %>
            <tr>
                <td><%= user.customer.name %></td>
                <td><%= user.email %></td>
                <td><%= user.customer.gender %></td>
            </tr>
        <% }); %>
    </tbody>
</table>

</body>
</html>