npx sequelize-cli model:generate --name User --attributes email:string,username:string,password:string,dateOfBirth:date,role:string

npx sequelize-cli model:generate --name Customer --attributes name:string,address:date,phoneNumber:string,balance:integer,UserId:integer


npx sequelize-cli model:generate --name Category --attributes name:string

npx sequelize-cli model:generate --name Product --attributes name:string,price:integer,stock:integer,description:string,imageURL:string,CategoryId:integer

npx sequelize-cli model:generate --name Order --attributes CustomerId:integer,ProductId:integer

npx sequelize-cli migration:generate --name add-gender-to-customer

npx sequelize-cli seed:generate --name demo-user
npx sequelize-cli seed:generate --name demo-customer
npx sequelize-cli seed:generate --name demo-category
npx sequelize-cli seed:generate --name demo-product
npx sequelize-cli seed:generate --name demo-order
